{
  "title": "Layout unificado de envio cobranca",
  "type": "object",
  "properties": {
    "nome": {
      "title": "Nome",
      "type": "string",
      "maxLength": 30,
      "default": ""
    },
    "cnpjcpf": {
      "title": "CNPJ/CPF",
      "type": "string",
      "pattern": "^(|\d{11}|\d{14})$",
      "maxLength": 14,
      "default": "",
      "format": "cnpjcpf"
    },
    "data": {
      "title": "Data de geração",
      "type": "string",
      "format": "date",
      "default": ""
    },
    "hora": {
      "title": "Horário de geração",
      "type": "string",
      "format": "time",
      "default": ""
    },
    "banco": {
      "title": "Banco",
      "type": "string",
      "enum": [
        "Itaú",
        "Bradesco",
        "Santader",
        "Caixa",
        "BB",
        "Daycoval"
      ]
    },
    "agencia": {
      "title": "Agência",
      "type": "integer",
      "default": 0
    },
    "conta": {
      "title": "Conta",
      "type": "integer",
      "default": 0
    },
    "registros": {
      "title": "Registros",
      "type": "array",
      "minItems": 1,
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "nome": {
            "title": "Nome",
            "type": "string",
            "maxLength": 40
          },
          "operacao": {
            "title": "Operação",
            "type": "string",
            "enum": [
              "Entrada",
              "Baixa",
              "Abatimento",
              "Prorrogação",
              "Protestar",
              "Não Protestar"
            ],
            "default": "Entrada"
          },
          "duplicata": {
            "title": "Duplicata",
            "type": "string",
            "maxLength": 10,
            "default": ""
          },
          "emissao": {
            "title": "Data de Emissão",
            "type": "string",
            "format": "date",
            "default": ""
          },
          "vencimento": {
            "title": "Data de Vencimento",
            "type": "string",
            "format": "date",
            "default": ""
          },
          "valor": {
            "title": "Valor (R$)",
            "type": "number",
            "multipleOf": 0.01,
            "minimum": 0,
            "maximum": 99999999999.99,
            "default": 0
          }
        }
      }
    }
  }
}
